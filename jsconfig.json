{
  "compilerOptions": {
    "checkJs": true,
    "strict": true,
    "module": "ES2022",
    "moduleResolution": "node",
    "target": "ES2022",
    "lib": ["ES2022"],
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*.js"],
  "exclude": ["node_modules", "spec"]
}
